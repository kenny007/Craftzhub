@model GettingStartedWithKnockout.Models.Registration.RegistrationVm
@{
    ViewBag.Title = "Registration";
    Layout = "~/Views/Shared/_Layout.cshtml";
 }



@section JavascriptInHead{
<script src="~/Scripts/Courses/courses-controller.js"></script>
<script src="~/Scripts/Instructors/instructors-controller.js"></script>   
<script type="text/javascript">
    registrationModule.factory('bootstrappedData', function () {
        return {
            courses:@Html.Raw(Model.Courses),
            instructors:@Html.Raw(Model.Instructors)
             };
     });    
</script>
}

<div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Registration", "Index", "Registration", null, new { @class = "brand" })
                <div class="nav-collapse collapse">
                    <ul class="nav">
                        <li><a href="/Registration/Courses">Browse Catalog</a></li>
                        <li><a href="/Registration/Instructors">Browse Instructors</a></li>
                    </ul>
                </div>
            </div>        
        </div>
     <div ng-view></div>

